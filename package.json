{
    "name": "wei-tool",
    "private": false,
    "version": "1.0.0",
    "description": "wei-tool",
    "license": "MIT",
    "auther": "forguo forguo@qq.com",
    "keywords": [
        "wei-tool",
        "js-tool"
    ],
    "files": [
        "lib/*"
    ],
    "main": "./lib/wei-tool.umd.js",
    "module": "./lib/wei-tool.es.js",
    "exports": {
        ".": {
            "import": "./lib/wei-tool.es.js",
            "require": "./lib/wei-tool.umd.js"
        }
    },
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "release": "standard-version",
        "release:alpha": "standard-version --prerelease alpha",
        "release:rc": "standard-version --prerelease rc",
        "release:major": "npm run release -- --release-as major",
        "release:minor": "npm run release -- --release-as minor",
        "release:patch": "npm run release -- --release-as patch"
    },
    "homepage": "https://github.com/wforguo/wei-tool.git",
    "repository": {
        "type": "git",
        "url": "https://github.com/wforguo/wei-tool.git"
    },
    "dependencies": {},
    "devDependencies": {
        "@types/node": "^18.0.0",
        "@vitejs/plugin-vue": "^2.3.3",
        "commitizen": "^4.2.2",
        "cz-conventional-changelog": "^3.3.0",
        "standard-version": "^9.1.0",
        "typescript": "^4.5.4",
        "vite": "^2.9.9",
        "vue": "^3.2.37"
    },
    "config": {
        "commitizen": {
            "path": "cz-conventional-changelog"
        }
    },
    "engines": {
        "node": ">=14.19.0"
    }
}
